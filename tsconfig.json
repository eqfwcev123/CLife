{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./build",
    "watch": true,
    "typeRoots": ["./tyopes", "./node_modules/@types"]
  },
  "exclude": ["node_modules", "types"]
}
